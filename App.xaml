            <GroupBox x:Key="FilmBilgileri" Header="FİLM BİLGİLERİ" Template="{StaticResource TintedGroupBox}">
                <DataGrid
                    x:Name="DgMovieDetails"
                    AutoGenerateColumns="False"
                    DataContext="{DynamicResource Film}"
                    IsReadOnly="True"
                    ItemsSource="{Binding Mode=OneWay, XPath=/root}"
                    RowHeight="150"
                    VirtualizingStackPanel.IsVirtualizing="True"
                    VirtualizingStackPanel.VirtualizationMode="Recycling">
                    <DataGrid.Resources>
                        <XmlDataProvider x:Key="Film" IsAsynchronous="True" IsInitialLoadEnabled="True" />
                    </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTextColumn Binding="{Binding XPath=/root/movie/@title}" Header="AD" />
                        <DataGridTextColumn Binding="{Binding XPath=/root/movie/@language}" Header="DİL" Width="60">
                            <DataGridTextColumn.ElementStyle>
                                <Style>
                                    <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
                                    <Setter Property="TextBlock.TextAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Binding="{Binding XPath=/root/movie/@year}" Header="YIL" />
                        <DataGridTextColumn Binding="{Binding XPath=/root/movie/@runtime}" Header="SÜRE" />
                        <DataGridTextColumn Binding="{Binding XPath=/root/movie/@director}" Header="YÖNETMEN" />
                        <DataGridTextColumn Binding="{Binding XPath=/root/movie/@country}" Header="ÜLKE" />
                        <DataGridTextColumn Binding="{Binding XPath=/root/movie/@actors}" Header="OYUNCULAR" Width="100">
                            <DataGridTextColumn.ElementStyle>
                                <Style>
                                    <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
                                    <Setter Property="TextBlock.TextAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Binding="{Binding XPath=/root/movie/@plot}" Header="ÖZET" Width="150">
                            <DataGridTextColumn.ElementStyle>
                                <Style>
                                    <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
                                    <Setter Property="TextBlock.TextAlignment" Value="Center" />
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTemplateColumn Header="RESİM">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <Image Width="80" Source="{Binding IsAsync=True, XPath=/root/movie/@poster}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                    </DataGrid.Columns>
                </DataGrid>
            </GroupBox>
            <ContextMenu x:Key="DataGridFilmlerHeaderContext">
                <MenuItem Header="{Binding Mode=OneWay, Path=Columns[4].Header, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type DataGrid}}}" IsCheckable="true" IsChecked="False">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="Checked">
