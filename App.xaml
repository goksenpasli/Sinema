<Application
    x:Class="Sinema.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/interactivedesigner/2006"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ice="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    xmlns:local="clr-namespace:Sinema"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    StartupUri="GirişWindow.xaml"
    mc:Ignorable="d">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/PresentationFramework.Aero,Version=4.0.0.0,Culture=neutral,PublicKeyToken=31bf3856ad364e35,processorArchitecture=MSIL;component/themes/Aero.NormalColor.xaml" />
                <ResourceDictionary Source="Icons/Icon.xaml" />
                <ResourceDictionary Source="Icons/Common.xaml" />
                <ResourceDictionary Source="Icons/DataGrid.xaml" />
                <ResourceDictionary Source="Icons/ScrollBar.xaml" />
                <ResourceDictionary Source="Icons/Slider.xaml" />
                <ResourceDictionary Source="Icons/CheckBox.xaml" />
                <ResourceDictionary Source="Icons/ComboBox.xaml" />
                <ResourceDictionary Source="Icons/HeaderedItemsControl.xaml" />
                <ResourceDictionary Source="Icons/RadioButton.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <local:IntToBrushConverter x:Key="IntToBrushConverter" />
            <local:IntToColorConverter x:Key="IntToColorConverter" />
            <local:SeansTarihGeçtiConverter x:Key="SeansTarihGeçtiConverter" />
            <local:PathToUriConverter x:Key="PathToUriConverter" />
            <local:SeansTarihGeçtiVisibilityConverter x:Key="SeansTarihGeçtiVisibilityConverter" />
            <Style TargetType="{x:Type local:Resizer}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type local:Resizer}">
                            <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="{x:Type local:ResizablePanel}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type local:ResizablePanel}">
                            <Grid DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="5" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="5" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="5" />
                                </Grid.RowDefinitions>
                                <local:Resizer
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Width="3"
                                    Height="3"
                                    Background="DarkBlue"
                                    Cursor="SizeNWSE"
                                    ThumbDirection="TopLeft" />

                                <local:Resizer
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Width="3"
                                    Background="DarkBlue"
                                    Cursor="SizeWE"
                                    ThumbDirection="Left" />

                                <local:Resizer
                                    Grid.Row="2"
                                    Grid.Column="0"
                                    Width="3"
                                    Height="3"
                                    Background="DarkBlue"
                                    Cursor="SizeNESW"
                                    ThumbDirection="BottomLeft" />

                                <local:Resizer
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Height="3"
                                    Background="DarkBlue"
                                    Cursor="SizeNS"
                                    ThumbDirection="Bottom" />

                                <local:Resizer
                                    Grid.Row="2"
                                    Grid.Column="2"
                                    Width="3"
                                    Height="3"
                                    Background="DarkBlue"
                                    Cursor="SizeNWSE"
                                    ThumbDirection="BottomRight" />

                                <local:Resizer
                                    Grid.Row="1"
                                    Grid.Column="2"
                                    Width="3"
                                    Background="DarkBlue"
                                    Cursor="SizeWE"
                                    ThumbDirection="Right" />

                                <local:Resizer
                                    Grid.Row="0"
                                    Grid.Column="2"
                                    Width="3"
                                    Height="3"
                                    Background="DarkBlue"
                                    Cursor="SizeNESW"
                                    ThumbDirection="TopRight" />

                                <local:Resizer
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Height="3"
                                    Background="DarkBlue"
                                    Cursor="SizeNS"
                                    ThumbDirection="Top" />

                                <ContentPresenter
                                    Content="{TemplateBinding Content}"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="{TemplateBinding Padding}" />
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style x:Key="DataGridTextBoxUpperStyle" TargetType="{x:Type TextBox}">
                <Setter Property="CharacterCasing" Value="Upper" />
            </Style>
            <DataTemplate x:Key="Toplam">
                <c1:C1NumericBox
                    Culture="tr-TR"
                    FontWeight="Bold"
                    Foreground="Red"
                    Format="C"
                    IsReadOnly="True"
                    ShowButtons="False"
                    Value="{Binding}" />
            </DataTemplate>
            <Style TargetType="{x:Type TabItem}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TabItem}">
                            <Border
                                x:Name="Border"
                                Margin="2,0"
                                BorderBrush="Gainsboro"
                                BorderThickness="1,1,1,0"
                                CornerRadius="9,9,0,0">
                                <ContentPresenter
                                    x:Name="ContentSite"
                                    Margin="2,2"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    ContentSource="Header" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter TargetName="Border" Property="Background">
                                        <Setter.Value>
                                            <LinearGradientBrush ice:Freeze="True" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                                                <GradientStop ice:Freeze="True" Offset="1" Color="#E561A5F7" />
                                                <GradientStop ice:Freeze="True" Color="#E561A5F7" />
                                                <GradientStop Offset="0.5" Color="#E50074FF" />
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>
                                <Trigger Property="IsSelected" Value="False">
                                    <Setter TargetName="Border" Property="Background" Value="GhostWhite" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="{x:Type TextBlock}">
                <Setter Property="VerticalAlignment" Value="Center" />
                <Setter Property="TextTrimming" Value="WordEllipsis" />
            </Style>
            <Style x:Key="PasifGrid" TargetType="{x:Type DataGridRow}">
                <Setter Property="IsEnabled" Value="True" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding AktifMi, UpdateSourceTrigger=PropertyChanged}" Value="false">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="GridSeansStyle" TargetType="{x:Type DataGridRow}">
                <Setter Property="Background" Value="LightGreen" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding FilmSaati, Converter={StaticResource SeansTarihGeçtiConverter}}" Value="false">
                        <Setter Property="Background" Value="Red" />
                    </DataTrigger>
                    <MultiDataTrigger>
                        <MultiDataTrigger.Conditions>
                            <Condition Binding="{Binding FilmSaati, Converter={StaticResource SeansTarihGeçtiConverter}}" Value="false" />
                            <Condition Binding="{Binding ElementName=CbSeans, Path=IsChecked}" Value="true" />
                        </MultiDataTrigger.Conditions>
                        <Setter Property="Visibility" Value="Collapsed" />
                    </MultiDataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="GridSeansEklemeEkranıStyle" TargetType="{x:Type DataGridRow}">
                <Setter Property="Background" Value="LightGreen" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding FilmSaati, Converter={StaticResource SeansTarihGeçtiConverter}}" Value="false">
                        <Setter Property="Background" Value="Red" />
                    </DataTrigger>
                    <MultiDataTrigger>
                        <MultiDataTrigger.Conditions>
                            <Condition Binding="{Binding FilmSaati, Converter={StaticResource SeansTarihGeçtiConverter}}" Value="false" />
                        </MultiDataTrigger.Conditions>
                        <Setter Property="Visibility" Value="Collapsed" />
                    </MultiDataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="GridSalonDurumlarıStyle" TargetType="{x:Type c1:DataGridRowPresenter}">
                <Setter Property="Background" Value="LightGreen" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Row.DataItem.FilmSaati, Converter={StaticResource SeansTarihGeçtiConverter}}" Value="false">
                        <Setter Property="Background" Value="Red" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="ComboboxKoltukStyle" BasedOn="{StaticResource {x:Type ComboBoxItem}}" TargetType="{x:Type ComboBoxItem}">
                <Setter Property="IsEnabled" Value="True" />
                <Setter Property="ToolTipService.ShowOnDisabled" Value="True" />
                <Setter Property="ToolTip">
                    <Setter.Value>
                        <StackPanel>
                            <TextBlock Text="{Binding KoltukTipleri.KoltukTipi, StringFormat=Koltuk Tipi: \{0\}}" />
                            <TextBlock Text="{Binding KoltukTipleri.KoltukFiyati, StringFormat=Koltuk Fiyatı: \{0:C\}}" />
                            <ListView Margin="5" ItemsSource="{Binding Musteriler}">
                                <ListView.ItemContainerStyle>
                                    <Style TargetType="{x:Type ListViewItem}">
                                        <Setter Property="Visibility" Value="{Binding Path=Seanslar.FilmSaati, Converter={StaticResource SeansTarihGeçtiVisibilityConverter}}" />
                                    </Style>
                                </ListView.ItemContainerStyle>
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn>
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Ellipse Width="16" Height="16" Fill="{Binding Seanslar.Renk, Converter={StaticResource IntToColorConverter}}" />
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="ADI" DisplayMemberBinding="{Binding MusteriAdi}" />
                                        <GridViewColumn Header="SOYADI" DisplayMemberBinding="{Binding MusteriSoyadi}" />
                                        <GridViewColumn Header="FİLM" DisplayMemberBinding="{Binding Seanslar.Filmler.FilmAdi}" />
                                        <GridViewColumn Header="TARİH" DisplayMemberBinding="{Binding Seanslar.FilmSaati}" />
                                        <GridViewColumn Header="TAHSİLAT" DisplayMemberBinding="{Binding Tahsilatlar/Tutar, StringFormat=\{0:C\}}" />
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </StackPanel>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style x:Key="ComboboxKullanıcıGirişStyle" BasedOn="{StaticResource {x:Type ComboBoxItem}}" TargetType="{x:Type ComboBoxItem}">
                <Setter Property="IsEnabled" Value="True" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding SifreEtkinMi}" Value="false">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="ComboboxSeansStyle" BasedOn="{StaticResource {x:Type ComboBoxItem}}" TargetType="{x:Type ComboBoxItem}">
                <Setter Property="IsEnabled" Value="True" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding FilmSaati, Converter={StaticResource SeansTarihGeçtiConverter}}" Value="false">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="ComboboxÜrünStyle" BasedOn="{StaticResource {x:Type ComboBoxItem}}" TargetType="{x:Type ComboBoxItem}">
                <Setter Property="IsEnabled" Value="True" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding UrunSatilabilirmi}" Value="false">
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="MüşteriEkleCellStyle" TargetType="{x:Type c1:DataGridCellPresenter}">
                <Setter Property="IsEnabled" Value="True" />
                <Style.Triggers>
                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=c1:DataGridRowPresenter}, Path=Row.DataItem.Seanslar.FilmSaati, Converter={StaticResource SeansTarihGeçtiConverter}}" Value="false">
                        <Setter Property="Background" Value="Red" />
                        <Setter Property="IsEnabled" Value="False" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Rectangle
                x:Key="ContextRectangle"
                Width="20"
                Height="20"
                x:Shared="False"
                Fill="{Binding KoltukRenk, Converter={StaticResource IntToBrushConverter}}" />
            <Style x:Key="GridButtonRenkleriStyle" BasedOn="{StaticResource {x:Type Button}}" TargetType="{x:Type Button}">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="1">
                        <Setter Property="Background" Value="Red" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="2">
                        <Setter Property="Background" Value="Green" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="3">
                        <Setter Property="Background" Value="Blue" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="4">
                        <Setter Property="Background" Value="Cyan" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="5">
                        <Setter Property="Background" Value="Magenta" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="6">
                        <Setter Property="Background" Value="Yellow" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="7">
                        <Setter Property="Background" Value="Orange" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="8">
                        <Setter Property="Background" Value="Purple" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="9">
                        <Setter Property="Background" Value="Lime" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding KoltukTipleri.KoltukRenk, Mode=OneWay}" Value="10">
                        <Setter Property="Background" Value="DarkCyan" />
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style TargetType="{x:Type TextBox}">
                <Setter Property="CharacterCasing" Value="Upper" />
                <Setter Property="Validation.ErrorTemplate">
                    <Setter.Value>
                        <ControlTemplate>
                            <Grid>
                                <Border>
                                    <AdornedElementPlaceholder x:Name="adorner" />
                                </Border>
                                <Canvas
                                    Width="15.27"
                                    Height="15.3"
                                    Margin="0,-7.5,-7.5,0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Top"
                                    ToolTip="{Binding ElementName=adorner, Path=AdornedElement.(Validation.Errors).CurrentItem.ErrorContent}">
                                    <Ellipse
                                        Canvas.Left="0"
                                        Canvas.Top="0"
                                        Width="15.27"
                                        Height="15.3"
                                        Fill="#FFFF5000"
                                        Stretch="Fill"
                                        Stroke="#FFFF0000"
                                        StrokeLineJoin="Round" />
                                    <Rectangle
                                        Canvas.Left="2.27"
                                        Canvas.Top="6.17"
                                        Width="10.87"
                                        Height="3.25"
                                        Fill="#FFFFFFFF"
                                        Stretch="Fill"
                                        Stroke="#FFFFFFFF"
                                        StrokeLineJoin="Round" />
                                </Canvas>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Setter Property="BorderBrush" Value="MediumSeaGreen" />
                <Setter Property="Background">
                    <Setter.Value>
                        <LinearGradientBrush MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                            <GradientStop Color="White" />
                            <GradientStop Offset="1" Color="#FE00B6E8" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="VerticalContentAlignment" Value="Center" />
            </Style>
            <Style TargetType="{x:Type PasswordBox}">
                <Setter Property="Validation.ErrorTemplate">
                    <Setter.Value>
                        <ControlTemplate>
                            <Grid>
                                <Border>
                                    <AdornedElementPlaceholder x:Name="adorner" />
                                </Border>
                                <Canvas
                                    Width="15.27"
                                    Height="15.3"
                                    Margin="0,-7.5,-7.5,0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Top"
                                    ToolTip="{Binding ElementName=adorner, Path=AdornedElement.(Validation.Errors).CurrentItem.ErrorContent}">
                                    <Ellipse
                                        Canvas.Left="0"
                                        Canvas.Top="0"
                                        Width="15.27"
                                        Height="15.3"
                                        Fill="#FFFF5000"
                                        Stretch="Fill"
                                        Stroke="#FFFF0000"
                                        StrokeLineJoin="Round" />
                                    <Rectangle
                                        Canvas.Left="2.27"
                                        Canvas.Top="6.17"
                                        Width="10.87"
                                        Height="3.25"
                                        Fill="#FFFFFFFF"
                                        Stretch="Fill"
                                        Stroke="#FFFFFFFF"
                                        StrokeLineJoin="Round" />
                                </Canvas>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Setter Property="BorderBrush" Value="MediumSeaGreen" />
                <Setter Property="Background">
                    <Setter.Value>
                        <LinearGradientBrush MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                            <GradientStop Color="White" />
                            <GradientStop Offset="1" Color="#FE00B6E8" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="VerticalContentAlignment" Value="Center" />
            </Style>
            <ControlTemplate x:Key="TintedGroupBox" TargetType="{x:Type GroupBox}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Border
                        Grid.Row="0"
                        BorderBrush="Black"
                        BorderThickness="1"
                        CornerRadius="2,2,0,0">
                        <Border.Background>
                            <LinearGradientBrush ice:Freeze="True" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                                <GradientStop ice:Freeze="True" Offset="1" Color="#E561A5F7" />
                                <GradientStop ice:Freeze="True" Color="#E561A5F7" />
                                <GradientStop Offset="0.5" Color="#E50074FF" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter Margin="4" ContentSource="Header" RecognizesAccessKey="True" />
                    </Border>
                    <Border
                        Grid.Row="1"
                        BorderBrush="Black"
                        BorderThickness="1,0,1,3"
                        CornerRadius="0,0,2,2">
                        <Border.Background>
                            <LinearGradientBrush ice:Freeze="True" StartPoint="0.5,0" EndPoint="0.5,1">
                                <GradientStop ice:Freeze="True" Offset="1" Color="#FE00B6E8" />
                                <GradientStop ice:Freeze="True" Color="#FF48F3CA" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter Margin="4" />
                    </Border>
                </Grid>
            </ControlTemplate>
            <Style x:Key="SatışProgressBarStyle" TargetType="ProgressBar">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="ProgressBar">
                            <Border>
                                <Grid x:Name="PART_Track">
                                    <Rectangle
                                        x:Name="PART_Indicator"
                                        HorizontalAlignment="Left"
                                        Fill="Black"
                                        Stroke="White"
                                        StrokeThickness="3" />
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Orientation" Value="Vertical">
                                    <Setter TargetName="PART_Track" Property="LayoutTransform">
                                        <Setter.Value>
                                            <RotateTransform Angle="-90" />
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
